{
  "$schema": "vscode://schemas/color-theme",
  "name": "Earthlings Theme",
  "type": "Dark",
  "colorSpaceName": "sRGB",
  "colors": {
    "foreground": "#CCCCCC",
    "activityBar.foreground": "#f1e8cf",
    "statusBar.foreground": "#f1e8cf",
    "titleBar.activeForeground": "#f1e8cf",
    "focusBorder": "#454545",
    "sideBar.border": "#1b1b1b",
    "statusBar.border": "#00000060",
    "sideBar.background": "#202020",
    "titleBar.border": "#00000040",
    "activityBar.border": "#bbbbbb10",
    "editor.background": "#252525",
    "editor.lineHighlightBorder": "#4545451f",
    "editor.lineHighlightBackground": "#463e3e2d",
    "editorLineNumber.activeForeground": "#685e5e",
    "editorLineNumber.foreground": "#5f554b",
    "editorIndentGuide.background": "#45454550",
    "editorGroup.border": "#1b1b1b",
    "editorGroupHeader.tabsBorder": "#1b1b1b",
    "editorGroup.focusedEmptyBorder": "#1b1b1b",
    "scrollbar.shadow": "#1b1b1b",
    "tab.border": "#1b1b1b90",
    "peekView.border": "#454545",
    "panel.border": "#1b1b1b",
    "panel.background": "#2D2D2D",
    "panelTitle.activeBorder": "#E6E6E6",
    "badge.background": "#656570",
    "statusBarItem.remoteBackground": "#0000",
    "statusBarItem.remoteForeground": "#CCC"
  },
  "tokenColors": [
    {
			"scope": [
				"meta.embedded",
				"source.groovy.embedded"
			],
			"settings": {
				"foreground": "#cacaca"
			}
		},
		{
			"scope": "emphasis",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "strong",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "header",
			"settings": {
				"foreground": "#5e756e"
			}
		},
		{
			"scope": "comment",
			"settings": {
				"foreground": "#5e756e"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#3CA5CE"
			}
		},
		{
			"scope": [
				"constant.numeric",
				"entity.name.operator.custom-literal.number",
				"variable.other.enummember",
				"keyword.operator.plus.exponent",
				"keyword.operator.minus.exponent"
			],
			"settings": {
				"foreground": "#0aaeb3"
			}
		},
		{
			"scope": "constant.regexp",
			"settings": {
				"foreground": "#0aaeb3"
			}
		},
		{
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#3CA5CE"
			}
		},
		{
			"scope": "entity.name.tag.css",
			"settings": {
				"foreground": "#d7ba7d"
			}
		},
		{
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#9BC6CF"
			}
		},
		{
			"scope": [
				"entity.other.attribute-name.class.css",
				"entity.other.attribute-name.class.mixin.css",
				"entity.other.attribute-name.id.css",
				"entity.other.attribute-name.parent-selector.css",
				"entity.other.attribute-name.pseudo-class.css",
				"entity.other.attribute-name.pseudo-element.css",
				"source.css.less entity.other.attribute-name.id",
				"entity.other.attribute-name.attribute.scss",
				"entity.other.attribute-name.scss"
			],
			"settings": {
				"foreground": "#d7ba7d"
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#f75341"
			}
		},
		{
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#3CA5CE"
			}
		},
		{
			"scope": "markup.heading",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#3CA5CE"
			}
		},
		{
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#0aaeb3"
			}
		},
		{
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#f75341"
			}
		},
		{
			"scope": "markup.changed",
			"settings": {
				"foreground": "#3CA5CE"
			}
		},
		{
			"scope": "punctuation.definition.quote.begin.markdown",
			"settings": {
				"foreground": "#a5ce3c"
			}
		},
		{
			"scope": "punctuation.definition.list.begin.markdown",
			"settings": {
				"foreground": "#3CA5CE"
			}
		},
		{
			"scope": "markup.inline.raw",
			"settings": {
				"foreground": "#f75341"
			}
		},
		{
			"name": "brackets of XML/HTML tags",
			"scope": "punctuation.definition.tag",
			"settings": {
				"foreground": "#808080"
			}
		},
		{
			"scope": [
				"meta.preprocessor",
				"entity.name.function.preprocessor"
			],
			"settings": {
				"foreground": "#3CA5CE"
			}
		},
		{
			"scope": "meta.preprocessor.string",
			"settings": {
				"foreground": "#f75341"
			}
		},
		{
			"scope": "meta.preprocessor.numeric",
			"settings": {
				"foreground": "#0aaeb3"
			}
		},
		{
			"scope": "meta.structure.dictionary.key.python",
			"settings": {
				"foreground": "#9BC6CF"
			}
		},
		{
			"scope": "meta.diff.header",
			"settings": {
				"foreground": "#3CA5CE"
			}
		},
		{
			"scope": "storage",
			"settings": {
				"foreground": "#3CA5CE"
			}
		},
		{
			"scope": "storage.type",
			"settings": {
				"foreground": "#3CA5CE"
			}
		},
		{
			"scope": [
				"storage.modifier",
				"keyword.operator.noexcept"
			],
			"settings": {
				"foreground": "#3CA5CE"
			}
		},
		{
			"scope": [
				"string",
				"entity.name.operator.custom-literal.string",
        "meta.embedded.assembly"
			],
			"settings": {
				"foreground": "#d4b46f"
			}
		},
		{
			"scope": "string.tag",
			"settings": {
				"foreground": "#d4b46f"
			}
		},
		{
			"scope": "string.value",
			"settings": {
				"foreground": "#d4b46f"
			}
		},
		{
			"scope": "string.regexp",
			"settings": {
				"foreground": "#d16969"
			}
		},
		{
			"name": "String interpolation",
			"scope": [
				"punctuation.definition.template-expression.begin",
				"punctuation.definition.template-expression.end",
				"punctuation.section.embedded"
			],
			"settings": {
				"foreground": "#3CA5CE"
			}
		},
		{
			"name": "Reset JavaScript string interpolation expression",
			"scope": [
				"meta.template.expression"
			],
			"settings": {
				"foreground": "#cacaca"
			}
		},
		{
			"scope": [
				"support.type.vendored.property-name",
				"support.type.property-name",
				"variable.css",
				"variable.scss",
				"variable.other.less",
				"source.coffee.embedded"
			],
			"settings": {
				"foreground": "#9BC6CF"
			}
		},
		{
			"scope": "keyword",
			"settings": {
				"foreground": "#3CA5CE"
			}
		},
		{
			"scope": "keyword.control",
			"settings": {
				"foreground": "#3CA5CE"
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#cacaca"
			}
		},
		{
			"scope": [
				"keyword.operator.new",
				"keyword.operator.expression",
				"keyword.operator.cast",
				"keyword.operator.sizeof",
				"keyword.operator.alignof",
				"keyword.operator.typeid",
				"keyword.operator.alignas",
				"keyword.operator.instanceof",
				"keyword.operator.logical.python",
				"keyword.operator.wordlike"
			],
			"settings": {
				"foreground": "#3CA5CE"
			}
		},
		{
			"scope": "keyword.other.unit",
			"settings": {
				"foreground": "#0aaeb3"
			}
		},
		{
			"scope": [
				"punctuation.section.embedded.begin.php",
				"punctuation.section.embedded.end.php"
			],
			"settings": {
				"foreground": "#3CA5CE"
			}
		},
		{
			"scope": "support.function.git-rebase",
			"settings": {
				"foreground": "#9BC6CF"
			}
		},
		{
			"scope": "constant.sha.git-rebase",
			"settings": {
				"foreground": "#0aaeb3"
			}
		},
		{
			"name": "coloring of the Java import and package identifiers",
			"scope": [
				"storage.modifier.import.java",
				"variable.language.wildcard.java",
				"storage.modifier.package.java"
			],
			"settings": {
				"foreground": "#cacaca"
			}
		},
		{
			"name": "this.self",
			"scope": "variable.language",
			"settings": {
				"foreground": "#3CA5CE"
			}
    },
    {
			"name": "Function declarations",
			"scope": [
				"entity.name.function",
				"support.function",
				"support.constant.handlebars",
				"source.powershell variable.other.member",
				"entity.name.operator.custom-literal" // See https://en.cppreference.com/w/cpp/language/user_literal
			],
			"settings": {
				"foreground": "#d4daae"
			}
		},
		{
			"name": "Types declaration and references",
			"scope": [
				"meta.return-type",
				"support.class",
				"support.type",
				"entity.name.type",
				"entity.name.namespace",
				"entity.other.attribute",
				"entity.name.scope-resolution",
				"entity.name.class",
				"storage.type.numeric.go",
				"storage.type.byte.go",
				"storage.type.boolean.go",
				"storage.type.string.go",
				"storage.type.uintptr.go",
				"storage.type.error.go",
				"storage.type.rune.go",
				"storage.type.cs",
				"storage.type.generic.cs",
				"storage.type.modifier.cs",
				"storage.type.variable.cs",
				"storage.type.annotation.java",
				"storage.type.generic.java",
				"storage.type.java",
				"storage.type.object.array.java",
				"storage.type.primitive.array.java",
				"storage.type.primitive.java",
				"storage.type.token.java",
				"storage.type.groovy",
				"storage.type.annotation.groovy",
				"storage.type.parameters.groovy",
				"storage.type.generic.groovy",
				"storage.type.object.array.groovy",
				"storage.type.primitive.array.groovy",
				"storage.type.primitive.groovy"
			],
			"settings": {
				"foreground": "#a5ce3c"
			}
		},
		{
			"name": "Types declaration and references, TS grammar specific",
			"scope": [
				"meta.type.cast.expr",
				"meta.type.new.expr",
				"support.constant.math",
				"support.constant.dom",
				"support.constant.json",
				"entity.other.inherited-class"
			],
			"settings": {
				"foreground": "#a5ce3c"
			}
		},
		{
			"name": "Control flow / Special keywords",
			"scope": [
				"keyword.control",
				"source.cpp keyword.operator.new",
				"keyword.operator.delete",
				"keyword.other.using",
				"keyword.other.operator",
				"entity.name.operator"
			],
			"settings": {
				"foreground": "#c16ab6"
			}
		},
		{
			"name": "Variable and parameter name",
			"scope": [
				"variable",
				"meta.definition.variable.name",
				"support.variable",
				"entity.name.variable"
			],
			"settings": {
				"foreground": "#9BC6CF"
			}
		},
		{
			"name": "Object keys, TS grammar specific",
			"scope": [
				"meta.object-literal.key"
			],
			"settings": {
				"foreground": "#9BC6CF"
			}
		},
		{
			"name": "CSS property value",
			"scope": [
				"support.constant.property-value",
				"support.constant.font-name",
				"support.constant.media-type",
				"support.constant.media",
				"constant.other.color.rgb-value",
				"constant.other.rgb-value",
				"support.constant.color"
			],
			"settings": {
				"foreground": "#0aaeb3"
			}
		},
		{
			"name": "Regular expression groups",
			"scope": [
				"punctuation.definition.group.regexp",
				"punctuation.definition.group.assertion.regexp",
				"punctuation.definition.character-class.regexp",
				"punctuation.character.set.begin.regexp",
				"punctuation.character.set.end.regexp",
				"keyword.operator.negation.regexp",
				"support.other.parenthesis.regexp"
			],
			"settings": {
				"foreground": "#0aaeb3"
			}
		},
		{
			"scope": [
				"constant.character.character-class.regexp",
				"constant.other.character-class.set.regexp",
				"constant.other.character-class.regexp",
				"constant.character.set.regexp"
			],
			"settings": {
				"foreground": "#d16969"
			}
		},
		{
			"scope": [
				"keyword.operator.or.regexp",
				"keyword.control.anchor.regexp"
			],
			"settings": {
				"foreground": "#cacaca"
			}
		},
		{
			"scope": "keyword.operator.quantifier.regexp",
			"settings": {
				"foreground": "#d7ba7d"
			}
		},
		{
			"scope": "constant.character",
			"settings": {
				"foreground": "#569cd6"
			}
		},
		{
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#d7ba7d"
			}
		},
		{
			"scope": "entity.name.label",
			"settings": {
				"foreground": "#C8C8C8"
			}
		}
  ],
  "semanticHighlighting": true
}